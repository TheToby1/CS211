#Created on 24 Feb 2015

class leaf(object):
    def __init__(self, letter):
        self.letter = letter
class node(leaf):
    def __init__(self, right, left):
        self.letter = None
        self.left = left
        self.right = right
        
class tree(object):
    def __init__(self, root, frequency):
        self.root = root
        self.frequency = frequency
        